{
  "name": "fanstackstico",
  "version": "1.0.0",
  "description": "It's fanstackstic!",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-dev",
    "build-dev": "npm run clean && webpack --config webpack/dev.config.js",
    "build-prod": "npm run clean && webpack --config webpack/prod.config.js",
    "dev": "webpack -w --config webpack/dev.config.js & nodemon build/server.js",
    "start": "node ./build/server.js",
    "clean": ". scripts/clean.sh",
    "clean-all": ". scripts/clean-all.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estebanmurchio/fanstackstico.git"
