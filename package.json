    "url": "https://github.com/estebanmurchio/fanstackstico/issues"
  },
  "homepage": "https://github.com/estebanmurchio/fanstackstico#readme",
  "dependencies": {
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "serialize-javascript": "^1.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
